interface memory;
  logic mem_en;
  logic rd_wr_mem;
  logic [7:0]mem_data;
  logic [1:0] mem_add;
  
  task memoperation(input mem_en, rd_wr_mem, mem_data, mem_add)
    
    while(mem_wn == 1) begin
    if (rd_wr_mem)
      mem[mem_add]=mem_data;
    else 
      mem_data=mem[mem_add];
    end
    
  endtask
  
endinterface

